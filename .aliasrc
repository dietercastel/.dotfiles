#Function to disown and redirect stdout/errout for easily starting with terminal.
function launchgui {
	( $1 &> /dev/null ) & 
}

alias keepass="launchgui keepass2"
alias intellij="launchgui /opt/intellij/bin/idea.sh"
alias screenfetch="~/Dropbox/Software/linux/screenfetch-dev"
alias ali="vim ~/.bash_aliases"
alias gs="git status"
alias gph="git push"
alias gpl="git pull"
alias gcam="git commit -am"
